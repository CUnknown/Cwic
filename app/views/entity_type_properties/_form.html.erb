<%= form_for(@entity_type_property) do |f| %>
  <%= render 'shared/error_messages', target: @entity_type_property %>

  <div class="field">
    <%= f.label :entity_type %>
    <%= f.text_field :entity_type %>
  </div>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :data_type %>
    <%= f.text_field :data_type %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
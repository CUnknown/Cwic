<div class="result-icon">
  <i class="icon-mimetype"></i>
</div>
<div class="info clearfix">
	<div class="column">
		column 1
	</div>
	<div class="column">
		column 2
	</div>
	<div class="column">
	  <h4><%= EntityImage.model_name.human(count: 2) %></h4>
    <dl>
      <% if result.entity_images.present? %>
        <div class="entity-images-container clearfix">
        <% result.entity_images.each do |eimage| %>
          <%= link_to(image_tag(eimage.image.thumb.url, {alt: eimage.title, title: eimage.title}), eimage.image.url) %>
        <% end %>
        </div>
      <% else %>
        <%= t('.no_entity_images') %>
      <% end %>
    </dl>
	</div>
</div>
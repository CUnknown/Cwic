<% content_for :page_title do %><%= t('.title_html', query: @query) %><% end %>

<% if @results.empty? %>
<p><%= t('.empty', default: :empty_objects, classes: t('.results')) %></p>
<% else %>

<div id="results">
  <%= render partial: 'result', collection: @results %>
</div>

<p>
  <%= page_entries_info(@raw_results, entry_name: t('.item')) %>
  <%= paginate(@raw_results) %>
</p>
<% end %>
